{
    "name": "alien-ui-boilerplate",
    "version": "0.0.1",
    "description": "React component library boilerplate using styled-components",
    "main": "index.js",
    "homepage": "https://github.com/trevonerd/alien-ui-boilerplate#readme",
    "scripts": {
        "clean": "rm -rf lib && rm -rf node_modules",
        "prepublishOnly": "npm run release",
        "release": "run-p build:components:dev build:components:prod build:icons:dev build:icons:prod",
        "build:components:prod": "NODE_ENV=production rollup -c",
        "build:components:dev": "NODE_ENV=development rollup -c",
        "build:icons:prod": "NODE_ENV=production rollup -c rollup.icons.config.js",
        "build:icons:dev": "NODE_ENV=development rollup -c rollup.icons.config.js",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook"
    },
    "author": "",
    "license": "ISC",
    "peerDependencies": {
        "react": ">= 16.x.x",
        "react-dom": ">= 16.x.x",
        "styled-components": ">= 4.x.x"
    },
    "devDependencies": {
        "@babel/core": "^7.11.6",
        "@babel/plugin-proposal-class-properties": "^7.5.5",
        "@babel/preset-env": "^7.7.7",
        "@babel/preset-react": "^7.0.0",
        "@storybook/addon-actions": "^5.2.0",
        "@storybook/addon-knobs": "^5.2.0",
        "@storybook/addon-links": "^5.2.0",
        "@storybook/addons": "^5.2.0",
        "@storybook/react": "^5.2.0",
        "@svgr/rollup": "^4.3.2",
        "babel-loader": "^8.0.6",
        "babel-plugin-styled-components": "^1.10.6",
        "npm-run-all": "^4.1.5",
        "react": "^16.9.0",
        "react-dom": "^16.9.0",
        "rollup": "^1.21.4",
        "rollup-plugin-babel": "^4.3.3",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-replace": "^2.2.0",
        "storybook-readme": "^5.0.8",
        "styled-components": "^4.3.2"
    },
    "dependencies": {}
}
